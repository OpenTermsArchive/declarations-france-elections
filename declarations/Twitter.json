{
  "name": "Twitter",
  "documents": {
    "Terms of Service": {
      "fetch": "https://twitter.com/fr/tos",
      "select": {
        "startBefore": ".twtr-type--headline-xxxxl",
        "endBefore": ".twtr-type--headline-xxxl"
      },
      "remove": [
        "[aria-hidden=\"true\"]",
        ".twtr-hidden--lg",
        ".ct01__background",
        ".ct07__sidebar",
        ".ct07__dropdown-wrapper"
      ]
    },
    "Privacy Policy": {
      "fetch": "https://twitter.com/fr/privacy",
      "select": {
        "startBefore": ".twtr-type--headline-xxxxl",
        "endBefore": "#old-policy"
      },
      "remove": [
        "[aria-hidden=\"true\"]",
        ".twtr-hidden--lg",
        ".ct01__background",
        ".ct17__background",
        ".ct07__sidebar",
        ".ct07__dropdown-wrapper"
      ]
    }
  }
}
